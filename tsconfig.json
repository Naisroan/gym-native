{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"*": [
				"src/*"
			],
			"tests": [
				"tests/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@hooks/*": [
				"src/hooks/*"
			],
			"@types/*": [
				"src/types/*"
			],
			"@helpers/*": [
				"src/helpers/*"
			],
			"@store/*": [
				"src/store/*"
			],
			"@routes/*": [
				"src/routes/*"
			],
			"@utils/*": [
				"src/utils/*"
			],
			"@views/*": [
				"src/views/*"
			]
		},
		"strictNullChecks": false,
		"moduleResolution": "Node"
	},
}